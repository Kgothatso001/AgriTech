def get_weather(api_key, location_data):
    """
    Retrieves the current weather data for a given location.

    Args:
        api_key (str): OpenWeatherMap API key
        location_data (dict): Dictionary containing city and country code

    Returns:
        dict: Weather data
    """
    city_name = location_data['city']
    country_code = location_data['country']

